{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://poker97.vercel.app/:path*",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "pppeople.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
